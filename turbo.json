{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env"],
  "tasks": {
    "dev:generate": {
      "inputs": ["prisma/schema.prisma"],
      "cache": false
    },
    "dev:migrate": {
      "inputs": ["prisma/schema.prisma"],
      "cache": false
    },
    "dev:push": {
      "inputs": ["prisma/schema.prisma"],
      "cache": false
    },
    "dev": {
      "persistent": true,
      "cache": false
    },
    "dev:build": {
      "dependsOn": ["^dev:generate"],
      "outputs": [".next/**"]
    },
    "ci:generate": {
      "outputs": []
    },
    "build": {
      "dependsOn": ["^build", "^dev:generate"],
      "outputs": [".next/**", ".expo/**"]
    },
    "build:web": {
      "dependsOn": ["^ci:generate", "build"],
      "outputs": [".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "build:dev:ios": {
      "outputs": []
    },
    "clean": {
      "cache": false
    },
    "//#clean": {
      "cache": false
    }
  },
  "globalEnv": [
    "NEXT_PUBLIC_SUPABASE_URL",
    "SUPABASE_SERVICE_ROLE_JWT",
    "NEXT_PUBLIC_SUPABASE_ANON_KEY",
    "DATABASE_URL",
    "VERCEL_URL",
    "NODE_ENV",
    "PORT"
  ]
}
